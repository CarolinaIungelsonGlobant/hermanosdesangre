<div class="col_none">
     <div class="col_div">
         <h3> Bookings for <%= @ong.name %></h3>
<table class="bookings_table">
  <tr>
    <th>Start</th>
    <th>End</th>
  </tr>
  
  <% @bookings.each do |booking| %>
  <tr>
    <td>
      <%= booking.start_time.strftime('%e %b %Y %H:%M%p') %>
    </td>
    <td>
      <%= (booking.calculate_end_time + 60).strftime('%e %b %Y %H:%M%p') %>
    </td>
    <td><%= link_to "Delete", ong_booking_path(@ong, booking), data: { confirm: 'Are you sure?' }, method: :delete %>
    </td>
    <td><%= link_to "Edit", edit_ong_booking_path(@ong, booking) %>
    </td>
  </tr>
  <% end %>
</table>

<td><%= link_to "New", new_ong_booking_path(@ong) %>
</td>
    </div></div>