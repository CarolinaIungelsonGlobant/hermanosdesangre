<h3>Reservas para <%= @ong.name %></h3>
  <table class="bookings_table">
    <tr>
      <th>Comienza</th>
      <th>Termina</th>
    </tr>
      <% turnos.each do |booking| %>
    <tr>
      <td>
        <%= booking.start_time.strftime('%e %b %Y %H:%M%p') %>
      </td>
      <td>
        <%= (booking.calculate_end_time + 60).strftime('%e %b %Y %H:%M%p') %>
      </td>
    <td><%= link_to "Borrar", ong_booking_path(@ong, booking), data: { confirm: 'EstÃ¡s seguro?' }, method: :delete %>
    </td>
    <td><%= link_to "Editar", edit_ong_booking_path(@ong, booking) %>
    </td>
    </tr>
    <% end %>
  </table>
  <div>
    <%= link_to "Nuevo Turno", new_ong_booking_path(@ong) %>
  </div>